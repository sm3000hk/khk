  jQuery(document).ready(function () {

    jQuery('.product_slide').slick({
      dots: false,
      infinite: false,
      speed: 300,
      slidesToShow: 3,
      slidesToScroll: 3,
      infinite: true,
      prevArrow: jQuery('.product_slide_nav.left.prev'),
