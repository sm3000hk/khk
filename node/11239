          breakpoint: 600,
          settings: {
            slidesToShow: 2,
            slidesToScroll: 2
          }
        },
        {
          breakpoint: 480,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1
          }
        }
        // You can unslick at a given breakpoint now by adding:
        // settings: "unslick"
        // instead of a settings object
      ]
    });


    if (jQuery('.node-product').attr('parent-cate') == "") {
      jQuery('#second-catagory').parent().hide();
    }

    jQuery('#first-catagory option').each(function () {
      jQuery(this).removeAttr("selected");

      if (jQuery('.node-product').attr('parent-cate') != "") {
        var cateid = jQuery('.node-product').attr('parent-cate');
      } else {
        var cateid = jQuery('.node-product').attr('sub-cate');
      }

      if (jQuery(this).attr("value") == cateid) {
        jQuery(this).prop("selected", true);
        jQuery('#first-catagory').change();
      }
    });


    jQuery('#second-catagory option').each(function () {
      jQuery(this).removeAttr("selected");

      if (jQuery(this).attr("value") == jQuery('.node-product').attr('sub-cate')) {
        jQuery(this).prop("selected", true);
        jQuery('#second-catagory').change();
      }
    });

    jQuery('#product option').each(function () {
      jQuery(this).removeAttr("selected");
      if (jQuery(this).attr("data-id") == jQuery('.node-product').attr('node-id')) {
        jQuery(this).prop("selected", true);
      }
    });

    

  });

</script>

