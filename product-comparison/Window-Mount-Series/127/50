<script>
    var categories = [];

    jQuery(".product-comparison--button.submit").click(function (e) {
        e.preventDefault();
        if (jQuery("input[name='selected_products[]']:checked").length > 3) {
            alert("Maximum 3 products selected.");
            return false;
        } else if (jQuery("input[name='selected_products[]']:checked").length < 2) {
            alert("Minimum 2 products selected before continue.");
            return false;
        } else {
            jQuery(".product-comparison--form").submit();
        }
        console.log(jQuery("input[name='selected_products[]']:checked").length);
    });
</script>

